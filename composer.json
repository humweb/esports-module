{
  "name": "humweb/esports-module",
  "type": "module",
  "license": "MIT",
  "authors": [
    {
      "name": "Ryan Shofner",
      "email": "ryun@humboldtweb.com"
    }
  ],
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "require": {
    "php": ">=7.0",
    "laravel/framework": "5.5.*",
    "spatie/laravel-medialibrary": "^6.0.0"

  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "squizlabs/php_codesniffer": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "Humweb\\Esports\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Humweb\\Esports\\ServiceProvider",
        "Humweb\\Esports\\EsportsRouteServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
